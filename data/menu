# RootMenu config for pekwm-0.1.1-pre4
Exec* Xterm:xterm -bg black -fg white &;
Exec* Emacs:xterm -bg black -fg white -title emacs -e emacs -nw &;

Submenu* Graphics;
	Exec* Gimp:gimp &;
SubmenuEnd* ;

Submenu* WwW;
	Exec* Galeon:galeon &;
	Exec* Irssi:xterm -bg black -fg white --title irssi -e irssi;
	Exec* Mozilla:mozilla &;
	Exec* Sylpheed:sylpheed &;
SubmenuEnd* ;

Submenu* Mm;
	Exec* Xmms:xmms &;
	Exec* MPlayer:mplayer &;
SubmenuEnd* ;

Submenu* Utils;
	Exec* xcalc:xcalc;
	Exec* xman:xman;
SubmenuEnd* ;

Submenu* PekWM;
	Reload* Reload;
	Restart* Restart;
	Exit* Exit;
	Submenu* Others;
		RestartOther* XTerm:xterm;
		RestartOther* Twm:twm;
	SubmenuEnd* ;
SubmenuEnd* ;
